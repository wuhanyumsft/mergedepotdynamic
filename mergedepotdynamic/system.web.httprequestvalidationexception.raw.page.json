eb.HttpRequestValidationException.yml","open_to_public_contributors":true,"api_name":["System.Web.HttpRequestValidationException","System.Web.HttpRequestValidationException..ctor"],"api_location":["System.Web.dll"],"topic_type":["apiref"],"api_type":["Assembly"],"f1_keywords":["System.Web.HttpRequestValidationException","System::Web::HttpRequestValidationException","System.Web.HttpRequestValidationException.#ctor","System::Web::HttpRequestValidationException::#ctor"],"dev_langs":["csharp"],"updated_at":"2017-03-01 01:37 AM","document_id":"ac00677f-7e98-a8a2-dd12-e14d50125ead","content_git_url":"https://github.com/TianqiZhang/ECMA2YamlTestRepo2/blob/master/fulldocset/xml/System.Web/HttpRequestValidationException.xml","layout":"Reference","_op_layout":"Reference","pagetype":"Reference","title":"HttpRequestValidationException class | Microsoft Docs","_op_ogTitle":"HttpRequestValidationException class","description":"The exception that is thrown when a potentially malicious input string is received from the client as part of the request data. This class cannot be inherited.\n","toc_asset_id":"_splitted/System.Web/toc.json","toc_rel":"_splitted/System.Web/toc.json","source_url":"","ms.assetid":"System.Web.HttpRequestValidationException","canonical_url":"https://ppe.docs.microsoft.com/en-us/mergedepotdynamic/system.web.httprequestvalidationexception","_op_canonicalUrl":"https://ppe.docs.microsoft.com/en-us/mergedepotdynamic/system.web.httprequestvalidationexception","fileRelativePath":"System.Web.HttpRequestValidationException.html"},"themesRelativePathToOutputRoot":"_themes/"}