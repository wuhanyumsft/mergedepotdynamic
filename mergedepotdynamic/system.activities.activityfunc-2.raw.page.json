\r\n  \t<h4 class=\"accented\"></h4>\r\n  \t<div class=\"cdlHolder\">\r\n  \t\t\t<div class=\"cdl\"><span class=\"lang-csharp\"><a class=\"xref\" href=\"system.activities.activitydelegate#System_Activities_ActivityDelegate_DisplayName\" data-linktype=\"relative-path\">DisplayName</a></span></div>\r\n  \t\t\t<div class=\"cdl\"><span class=\"lang-csharp\"><a class=\"xref\" href=\"system.activities.activitydelegate#System_Activities_ActivityDelegate_Handler\" data-linktype=\"relative-path\">Handler</a></span></div>\r\n  \t\t\t<div class=\"cdl\"><span class=\"lang-csharp\"><a class=\"xref\" href=\"system.activities.activitydelegate#System_Activities_ActivityDelegate_OnGetRuntimeDelegateArguments_System_Collections_Generic_IList_System_Activities_RuntimeDelegateArgument__\" data-linktype=\"relative-path\">OnGetRuntimeDelegateArguments(IList&lt;RuntimeDelegateArgument&gt;)</a></span></div>\r\n  \t\t\t<div class=\"cdl\"><span class=\"lang-csharp\"><a class=\"xref\" href=\"system.activities.activitydelegate#System_Activities_ActivityDelegate_ShouldSerializeDisplayName\" data-linktype=\"relative-path\">ShouldSerializeDisplayName()</a></span></div>\r\n  \t\t\t<div class=\"cdl\"><span class=\"lang-csharp\"><a class=\"xref\" href=\"system.activities.activitydelegate#System_Activities_ActivityDelegate_ToString\" data-linktype=\"relative-path\">ToString()</a></span></div>\r\n  \t\t\t<div class=\"cdl\"><span class=\"lang-csharp\"><a class=\"xref\" href=\"system.object#System_Object_Equals_System_Object_\" data-linktype=\"relative-path\">Equals(Object)</a></span></div>\r\n  \t\t\t<div class=\"cdl\"><span class=\"lang-csharp\"><a class=\"xref\" href=\"system.object#System_Object_Equals_System_Object_System_Object_\" data-linktype=\"relative-path\">Equals(Object,Object)</a></span></div>\r\n  \t\t\t<div class=\"cdl\"><span class=\"lang-csharp\"><a class=\"xref\" href=\"system.object#System_Object_GetHashCode\" data-linktype=\"relative-path\">GetHashCode()</a></span></div>\r\n  \t\t\t<